<%= render "layouts/errors", :object => @service %>

<%= form_for @service do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
  <%= f.label :price %><br>
  <%= f.text_field :price %><br>
  <%= f.submit %>
<% end %>
